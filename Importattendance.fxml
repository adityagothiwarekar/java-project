<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sample2.Import">
   <children>
      <ImageView fitHeight="403.0" fitWidth="610.0" layoutY="-3.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@image3.png" />
         </image>
      </ImageView>
      <Button fx:id="add" layoutX="113.0" layoutY="354.0" mnemonicParsing="false" onAction="#upload" prefHeight="36.0" prefWidth="132.0" style="-fx-background-color: red;" text="SELECT" textFill="#f4f4f4">
         <font>
            <Font name="Tahoma" size="18.0" />
         </font></Button>
      <Text fill="WHITE" layoutX="165.0" layoutY="333.0" strokeType="OUTSIDE" strokeWidth="0.0" text="    ADD CSV FILE" wrappingWidth="164.0">
         <font>
            <Font name="Yu Gothic Regular" size="18.0" />
         </font></Text>
      <TextArea fx:id="display" layoutX="65.0" layoutY="74.0" prefHeight="230.0" prefWidth="497.0" promptText="THE DATA OF CSV FILE WILL APPEAR HERE">
         <font>
            <Font size="20.0" />
         </font></TextArea>
      <Button fx:id="select" layoutX="268.0" layoutY="354.0" mnemonicParsing="false" onAction="#Select" prefHeight="26.0" prefWidth="122.0" style="-fx-background-color: red;" text="UPLOAD" textFill="WHITE">
