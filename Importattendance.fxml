      <Button fx:id="home" layoutX="518.0" layoutY="376.0" mnemonicParsing="false" onAction="#homebutton" prefHeight="20.0" prefWidth="54.0" style="-fx-background-color: BLACK;" text="HOME" textFill="WHITE" />
      <ImageView fitHeight="50.0" fitWidth="64.0" layoutX="461.0" layoutY="365.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@homebutton.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
